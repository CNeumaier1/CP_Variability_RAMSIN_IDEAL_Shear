! Coarse grid specifications

   IHTRAN   = 0,              ! 0-Cartesian, 1-Polar stereo
   DELTAX   = 100.,          ! X-Y grid spacing (meters)
   DELTAZ   = 0.,           ! Z grid spacing (set to 0. to use ZZ)
   DZRAT    = 1.02,           ! Vertical grid stretch ratio
   DZMAX    = 200.,          ! Maximum delta Z for vertical stretch
 
   ZZ       = 0.,           ! Vertical levels if DELTAZ = 0
       50.,   100.,   150.,   200.,   250.,   300.,   350.,
